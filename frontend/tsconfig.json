{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "baseUrl": "./src",
    "paths": {
      "*": ["node_modules/*", "*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
